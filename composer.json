{
    "name": "dms/php-password-strength-checker",
    "description": "A simple php password strength checker library",
    "version": "1.0.0",
    "keywords": [
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Son\\PasswordStrengthChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Son\\PasswordStrengthChecker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
